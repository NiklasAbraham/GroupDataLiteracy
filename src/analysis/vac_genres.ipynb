    "  },\n",
    "  {\n",
    "   \"metadata\": {},\n",
    "   \"cell_type\": \"code\",\n",
    "   \"source\": [\n",
    "    \"def plot_genre_drift(df, y_column, title, y_label):\\n\",\n",
    "    \"    sns.set_theme(style=\\\"whitegrid\\\")\\n\",\n",
    "    \"    df['year'] = pd.to_numeric(df['year'])\\n\",\n",
    "    \"\\n\",\n",
    "    \"    plt.figure(figsize=(12, 6))\\n\",\n",
    "    \"    sns.lineplot(\\n\",\n",
    "    \"        data=df,\\n\",\n",
    "    \"        x='year',\\n\",\n",
    "    \"        y=y_column,\\n\",\n",
    "    \"        hue='single_genre',\\n\",\n",
    "    \"        marker='o',\\n\",\n",
    "    \"        linewidth=1.5\\n\",\n",
    "    \"    )\\n\",\n",
    "    \"    plt.title(title, fontsize=16)\\n\",\n",
    "    \"    plt.xlabel('Año', fontsize=12)\\n\",\n",
    "    \"    plt.ylabel(y_label, fontsize=12)\\n\",\n",
    "    \"\\n\",\n",
    "    \"    plt.legend(title='Género', bbox_to_anchor=(1.05, 1), loc='upper left')\\n\",\n",
    "    \"    plt.tight_layout()\\n\",\n",
    "    \"    plt.show()\\n\",\n",
    "    \"\\n\",\n",
    "    \"\\n\",\n",
    "    \"plot_genre_drift(\\n\",\n",
    "    \"    df=drift_df,\\n\",\n",
    "    \"    y_column='cumulative_change',\\n\",\n",
    "    \"    title='Genre Cumulative Change Over Time',\\n\",\n",
    "    \"    y_label='Cumulative Cosine Distance'\\n\",\n",
    "    \")\\n\",\n",
    "    \"\\n\",\n",
    "    \"plot_genre_drift(\\n\",\n",
    "    \"    df=drift_df,\\n\",\n",
    "    \"    y_column='drift_distance',\\n\",\n",
    "    \"    title='Genre change velocity (How much each genre changed compared to previous year)',\\n\",\n",
    "    \"    y_label='Cosine Distance'\\n\",\n",
    "    \")\\n\"\n",
    "   ],\n",
