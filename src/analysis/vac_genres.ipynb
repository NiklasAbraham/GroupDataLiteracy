    "   \"outputs\": [],\n",
    "   \"execution_count\": 7\n",
    "  },\n",
    "  {\n",
    "   \"metadata\": {},\n",
    "   \"cell_type\": \"code\",\n",
    "   \"source\": [\n",
    "    \"print(f\\\"\\\\nLoading movie data from {DATA_DIR}...\\\")\\n\",\n",
    "    \"df = load_movie_data_limited(DATA_DIR, N_MOVIES_PER_YEAR, verbose=False)\\n\",\n",
    "    \"print(df.shape)\\n\",\n",
    "    \"print(df.info())\\n\"\n",
    "   ],\n",
    "   \"id\": \"810be36b647e13b4\",\n",
    "   \"outputs\": [],\n",
    "   \"execution_count\": null\n",
    "  },\n",
    "  {\n",
    "   \"metadata\": {\n",
    "    \"ExecuteTime\": {\n",
    "     \"end_time\": \"2025-11-14T08:41:02.164107Z\",\n",
    "     \"start_time\": \"2025-11-14T08:41:02.160372Z\"\n",
    "    }\n",
    "   },\n",
    "   \"cell_type\": \"code\",\n",
    "   \"source\": \"# For each genre, calculate the center in each year and substract from the previous year's center to get the velocity -> absolute value\",\n",
    "   \"id\": \"94bea1c58b686363\",\n",
    "   \"outputs\": [\n",
    "    {\n",
    "     \"name\": \"stdout\",\n",
    "     \"output_type\": \"stream\",\n",
    "     \"text\": [\n",
    "      \"(375, 22)\\n\"\n",
    "     ]\n",
    "    }\n",
    "   ],\n",
    "   \"execution_count\": 3\n",
    "  },\n",
    "  {\n",
    "   \"metadata\": {\n",
    "    \"ExecuteTime\": {\n",
    "     \"end_time\": \"2025-11-15T19:49:09.843941Z\",\n",
    "     \"start_time\": \"2025-11-15T19:49:09.839467Z\"\n",
    "    }\n",
    "   },\n",
    "   \"cell_type\": \"code\",\n",
    "   \"source\": [\n",
    "    \"def compute_cosine_distance(row):\\n\",\n",
    "    \"    if np.all(pd.isna(row['next_avg_embedding'])):\\n\",\n",
    "    \"        return np.nan\\n\",\n",
    "    \"\\n\",\n",
    "    \"    return cosine(row['avg_embedding'], row['next_avg_embedding'])\"\n",
    "   ],\n",
    "   \"id\": \"c8dc115d8a8c3dab\",\n",
    "   \"outputs\": [],\n",
    "   \"execution_count\": 3\n",
    "  },\n",
    "  {\n",
    "   \"metadata\": {},\n",
    "   \"cell_type\": \"code\",\n",
    "   \"source\": [\n",
    "    \"# remove Nan genres\\n\",\n",
    "    \"print(f\\\"Number of movies before cleaning: {df.shape[0]}\\\")\\n\",\n",
    "    \"df_cleaned = df.dropna(subset=['genre', 'year', 'embedding']).copy()\\n\",\n",
    "    \"df_cleaned = df_cleaned[df_cleaned['year'] >= START_YEAR]\\n\",\n",
    "    \"print(f\\\"Number of movies after cleaning: {df_cleaned.shape[0]}\\\")\\n\",\n",
    "    \"\\n\",\n",
    "    \"# group into single genres\\n\",\n",
    "    \"df_cleaned['genre_list'] = df_cleaned['genre'].str.split(', ')\\n\",\n",
    "    \"df_exploded = df_cleaned.explode('genre_list')\\n\",\n",
    "    \"df_exploded.rename(columns={'genre_list': 'single_genre'}, inplace=True)\\n\",\n",
    "    \"print(f\\\"Number of movies after exploding: {df_exploded.shape[0]}\\\")\\n\",\n",
    "    \"\\n\",\n",
    "    \"# filter to top X genres\\n\",\n",
    "    \"top_10_genres = df_exploded['single_genre'].value_counts().nlargest(TOP_X).index.tolist()\\n\",\n",
    "    \"df_exploded = df_exploded[df_exploded['single_genre'].isin(top_10_genres)].copy()\\n\",\n",
    "    \"print(f\\\"Top {TOP_X} genres: {df_exploded.shape[0]} total movies\\\")\\n\",\n",
    "    \"\\n\",\n",
    "    \"# calculate average embedding per year and genre\\n\",\n",
    "    \"grouped_embeddings = df_exploded.groupby(['year', 'single_genre'])['embedding'].apply(\\n\",\n",
    "    \"    lambda x: np.mean(np.vstack(x), axis=0)\\n\",\n",
    "    \")\\n\",\n",
    "    \"group_df = grouped_embeddings.reset_index(name='avg_embedding')\\n\",\n",
    "    \"group_df = group_df.sort_values(by=['single_genre', 'year']).copy()\\n\",\n",
    "    \"\\n\",\n",
    "    \"# shift embeddings -1 into \\\"next_avg_embeddings\\\" -> Embedding t and embedding t+1 -> calculate cosine distance\\n\",\n",
    "    \"group_df['next_avg_embedding'] = group_df.groupby('single_genre')['avg_embedding'].shift(-1)\\n\",\n",
    "    \"group_df['drift_distance'] = group_df.apply(compute_cosine_distance, axis=1)\\n\",\n",
    "    \"drift_df = group_df.dropna(subset=['drift_distance']).copy()\\n\",\n",
    "    \"\\n\",\n",
    "    \"# calculate cumulative change per genre\\n\",\n",
    "    \"drift_df['cumulative_change'] = drift_df.groupby('single_genre')['drift_distance'].cumsum()\"\n",
    "   ],\n",
