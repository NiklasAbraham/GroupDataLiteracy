    "   },\n",
    "   \"cell_type\": \"code\",\n",
    "   \"source\": [\n",
    "    \"import os\\n\",\n",
    "    \"import sys\\n\",\n",
    "    \"import numpy as np\\n\",\n",
    "    \"import pandas as pd\\n\",\n",
    "    \"from pathlib import Path\\n\",\n",
    "    \"from datetime import datetime\\n\",\n",
    "    \"\\n\",\n",
    "    \"import plt\\n\",\n",
    "    \"import seaborn as sns\\n\",\n",
    "    \"from scipy.spatial.distance import cosine\\n\",\n",
    "    \"\\n\",\n",
    "    \"from data_utils import load_movie_data, load_movie_data_limited\\n\",\n",
    "    \"\\n\",\n",
    "    \"BASE_DIR = Path.cwd().parent.parent\\n\",\n",
    "    \"SRC_DIR = BASE_DIR / 'src'\\n\",\n",
    "    \"sys.path.insert(0, str(SRC_DIR))\\n\",\n",
    "    \"\\n\",\n",
    "    \"DATA_DIR = str(BASE_DIR / \\\"data\\\")\\n\",\n",
    "    \"OUTPUT_DIR = None\\n\",\n",
    "    \"N_MOVIES_PER_YEAR = None\\n\",\n",
    "    \"RANDOM_SEED = 42\\n\",\n",
    "    \"START_YEAR = 19\\n\",\n",
    "    \"START_YEAR = 1960\\n\",\n",
    "    \"TOP_X = 5\\n\"\n",
    "   ],\n",
